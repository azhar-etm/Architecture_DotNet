        var checkBox1 = $('input[name="' + checkboxName + '"]');
        $(checkBox1).each(function () {
            if ($(this).is(':checked')) {
                $(this).addClass("selected");
            }
        });
        $(checkBox1).click(function () {
            if ($(this).is(':checked')) {
                $(this).addClass("selected");
            }
            else {
                $(this).removeClass("selected");
            }
        });
    }
