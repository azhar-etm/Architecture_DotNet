@{
    var Cookie_Inf = RMS_WEB.Controllers.cookieInfo.GetUserInfo(System.Web.HttpContext.Current.Request);
    string email = "";
    string userid = "";
    string mobile = "";
    string last_logon = "";
    string name = "";
    string addr1 = "", addr2 = "";
    string city = "", region = "";
    string country = "", pincode = "";
    string mail1 = "", mail2 = "";
    string phone1 = "", phone2 = "";

    if (Cookie_Inf.USERID != null && Cookie_Inf.USERID != "")
    {
        email = Cookie_Inf.EMAIL;
        name = Cookie_Inf.NAME;
        userid = Cookie_Inf.USERID;
        mobile = Cookie_Inf.MOBILE;
        last_logon = Cookie_Inf.LAST_LOGON;
        addr1 = Cookie_Inf.ADDR1;
        addr2 = Cookie_Inf.ADDR2;
        city = Cookie_Inf.CITY;
        region = Cookie_Inf.REGION;
        country = Cookie_Inf.COUNTRY;
        pincode = Cookie_Inf.PINCODE;
        mail1 = Cookie_Inf.MAIL1;
        mail2 = Cookie_Inf.MAIL2;
        phone1 = Cookie_Inf.PHONE1;
        phone2 = Cookie_Inf.PHONE2;
    }
    else
    {
        Response.Redirect("~/Login");
    }
}

